{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.3" }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/assets/image/$1"
        },
        {
            "src": "/(.*\\.php)",
            "dest": "/api/$1/"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
